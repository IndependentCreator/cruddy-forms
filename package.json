{
	"name": "cruddy-forms",
	"version": "0.1.1",
	"description": "HTML Form generator with typebox validation",
	"repository": {
		"type": "git",
		"url": "https://github.com/IndependentCreator/cruddy-forms"
	},
	"license": "MIT",
	"author": {
		"name": "IndependentCreator",
		"email": "independentcreator@protonmail.com"
	},
	"type": "module",
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/",
		"package.json",
		"LICENSE.md",
		"README.md"
	],
	"scripts": {
		"build": "tsup",
		"coverage": "vitest run --coverage",
		"format": "prettier \"**/*\" --ignore-unknown",
		"format:write": "pnpm format --write",
		"lint": "eslint src",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:package-json": "npmPkgJsonLint .",
		"lint:packages": "pnpm dedupe --check",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"pack": "pnpm pack --pack-destination ~",
		"publish": "node scripts/publish.js",
		"release": "node scripts/release.js",
		"release:rc": "node scripts/release.js --rc",
		"should-semantic-release": "should-semantic-release --verbose",
		"test": "vitest run",
		"test:watch": "vitest",
		"tsc": "tsc"
	},
	"lint-staged": {
		"*.{ts,cjs}": "eslint"
	},
	"dependencies": {
		"@sinclair/typebox": "0.30.4",
		"typebox-form-parser": "^0.1.6"
	},
	"devDependencies": {
		"@release-it/conventional-changelog": "^7.0.2",
		"@types/eslint": "^8.44.3",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.7.2",
		"@vitest/coverage-v8": "^0.34.5",
		"c8": "^8.0.1",
		"cspell": "^7.3.6",
		"eslint": "^8.50.0",
		"eslint-plugin-deprecation": "^2.0.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-jsdoc": "^46.8.2",
		"eslint-plugin-jsonc": "^2.9.0",
		"eslint-plugin-markdown": "^3.0.1",
		"eslint-plugin-n": "^16.1.0",
		"eslint-plugin-no-only-tests": "^3.1.0",
		"eslint-plugin-perfectionist": "^2.1.0",
		"eslint-plugin-regexp": "^1.15.0",
		"eslint-plugin-vitest": "^0.3.1",
		"eslint-plugin-yml": "^1.9.0",
		"happy-dom": "^12.2.1",
		"html-validate": "^8.4.1",
		"husky": "^8.0.3",
		"jsonc-eslint-parser": "^2.3.0",
		"knip": "^2.25.2",
		"lint-staged": "^14.0.1",
		"markdownlint": "^0.31.1",
		"markdownlint-cli": "^0.37.0",
		"msw": "^1.3.1",
		"npm-package-json-lint": "^7.0.0",
		"npm-package-json-lint-config-default": "^6.0.0",
		"prettier-plugin-curly": "^0.1.3",
		"prettier-plugin-packagejson": "^2.4.5",
		"release-it": "^16.2.0",
		"sentences-per-line": "^0.2.1",
		"should-semantic-release": "^0.1.1",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2",
		"vitest": "^0.34.5",
		"yaml-eslint-parser": "^1.2.2"
	},
	"packageManager": "pnpm@8.7.0",
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"provenance": true
	}
}
